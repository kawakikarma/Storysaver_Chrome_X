{
  "manifest_version": 2,
  "name": "Instagram Story Downloader",
  "description": "Download Instagram stories",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "https://www.instagram.com/*"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["content.js"]
    }
  ]
}
